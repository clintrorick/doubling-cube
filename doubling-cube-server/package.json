{
  "name": "doubling-cube-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint app.js",
    "start": "babel-node ./bin/www",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js"
  },
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "vars-on-top": 2,
    "no-undef": 2
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
